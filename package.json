{
  "name": "sparc",
  "version": "0.1.6",
  "main": "./dist",
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "build": "rimraf ./dist clean && tsc",
    "test": "NODE_ENV=test mocha test/**/*.spec.ts* --require ./register.js --bail",
    "test:clean": "NODE_ENV=test npm run build && mocha test/**/*.spec.ts* --require ./register.js --bail",
    "electron": "electron ./test/main",
    "prepublish": "git add . && git commit -am 'auto commit' && git push"
  },
  "dependencies": {
    "cross-spawn": "^6.0.5",
    "electron": "^5.0.6",
    "electron-is-dev": "^1.1.0",
    "electron-store": "^4.0.0",
    "serve": "^11.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/cross-spawn": "^6.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "chai": "^4.2.0",
    "concurrently": "^4.1.1",
    "mocha": "^6.1.4",
    "rimraf": "^2.6.3",
    "spectron": "^7.0.0",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  },
  "keywords": [
    "Electron",
    "Create React App",
    "React",
    "Knectron",
    "Bootstrap"
  ],
  "license": "ISC",
  "author": "Blujedis <blujedicorp@gmail.com>"
}
